{
  "name": "mail-template-parser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-prod": "mjml ./mail_templates/*.mjml -o ./mail_templates/html/ --config.minify true --config.beautify false && node copy.js",
    "watch": "concurrently --kill-others \"npm run live-reload-server\" \"npm run mjml-watch\"",
    "live-reload-server": "live-server --port=7786 ./mail_templates/html",
    "mjml-watch": "node index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/fs-extra": "^5.0.3",
    "@types/node": "^10.5.1",
    "concurrently": "^3.6.0",
    "fs-extra": "^6.0.1",
    "live-server": "^1.2.0",
    "mjml": "^4.1.0",
    "nodemon": "^1.17.5"
  }
}
